node ('win-slave') {
    def mvnHome
   stage('Preparation') {
      mvnHome = tool 'maven3'
   }
   stage('Build') {
      bat(/"${mvnHome}\bin\mvn" clean install)
   }
   stage('Results') {
      archive 'target/*.jar'
   }   
    stage('Copy Artifact') {
        node('master'){
            copyArtifacts filter: 'target/*.jar', fingerprintArtifacts: true, projectName: "${JOB_NAME}", selector: specific("${BUILD_NUMBER}"), target: 'artifacts'
        }
   } 
}
